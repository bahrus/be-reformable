{
  "name": "be-reformable",
  "version": "0.0.75",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "decorator",
    "behavior",
    "directive"
  ],
  "description": "be-reformable is a web component that progressively enhances the built-in form element",
  "main": "be-reformable.js",
  "module": "be-reformable.js",
  "exports": {
    ".": "./be-reformable.js",
    "./be-reformable.js": "./be-reformable.js"
  },
  "files": [
    "*.*"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "be-enhanced": "0.0.147",
    "be-hive": "0.0.227",
    "mount-observer": "0.0.39",
    "trans-render": "0.0.848"
  },
  "devDependencies": {
    "ssi-server": "0.0.1",
    "@playwright/test": "1.48.1"
  },
  "scripts": {
    "test": "playwright test",
    "serve": "python3 ./node_modules/ssi-server/ssi_server.py",
    "safari": "npx playwright wk http://localhost:8000",
    "update": "ncu -u && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-reformable.git"
  },
